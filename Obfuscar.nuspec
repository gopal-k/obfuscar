		<language>en-US</language>
		<tags>obfuscation,obfuscator,open source</tags>
	</metadata>
	<files>
		<file src="bin\release\obfuscar.console.exe" target="tools" />
		<file src="build\obfuscar.props" target="build" />
	</files>
</package>
